@article{Grindrod2010,
abstract = {We examine the importance of localized volcanism in resurfacing on Venus by analyzing the results of geologic mapping of a 12° × 12° area at the full resolution of Magellan SAR data. Resurfacing due to corona-, ridge-, and small volcano-related volcanism accounts for 27\%, 6\%, and 10\% respectively of the mapped area. Mapping at the resolution of Magellan data, rather than a regional scale, gives corona-related flow unit areas that can differ individually by almost an order of magnitude, with a total increase of 28\%, and more than three times as many identifiable units. A total of 2919 small volcanoes or vents less than 10 km in diameter were identified in the F-Map, with a mean diameter of 1.59 (s.d. = 1.08) km and densities of up to 36 small volcanoes per 50 km2. Taken together, coronae, ridge eruptions, and small volcanoes probably make a significant contribution to resurfacing on Venus.},
author = {Grindrod, Peter M. and Stofan, Ellen R. and Guest, John E.},
doi = {10.1029/2010GL043424},
file = {:Users/damo\_ma/Documents/Dropbox/work/Scientific\_Library/2010/Geophysical Research Letters/Grindrod, Stofan, Guest\_Volcanism and resurfacing on Venus at the full resolution of Magellan SAR data\_Geophysical Research Letters\_2010.pdf:pdf},
issn = {0094-8276},
journal = {Geophysical Research Letters},
keywords = {magellan,resurfacing,venus,volcanism},
mendeley-tags = {magellan,resurfacing,venus,volcanism},
month = aug,
number = {15},
pages = {1--5},
title = {{Volcanism and resurfacing on Venus at the full resolution of Magellan SAR data}},
url = {http://www.agu.org/pubs/crossref/2010/2010GL043424.shtml},
volume = {37},
year = {2010}
}
