file = {:Users/damo\_ma/Documents/Dropbox/work/Scientific\_Library/2010/Icarus/Merlin et al.\_Chemical and physical properties of the variegated Pluto and Charon surfaces\_Icarus\_2010.pdf:pdf},
issn = {00191035},
