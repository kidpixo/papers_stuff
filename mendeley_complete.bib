@article{Sunshine2009,
abstract = {The Moon is generally anhydrous, yet the Deep Impact spacecraft found the entire surface to be hydrated during some portions of the day. Hydroxyl (OH) and water (H2O) absorptions in the near infrared were strongest near the North Pole and are consistent with <0.5 weight percent H2O. Hydration varied with temperature, rather than cumulative solar radiation, but no inherent absorptivity differences with composition were observed. However, comparisons between data collected 1 week (a quarter lunar day) apart show a dynamic process with diurnal changes in hydration that were greater for mare basalts (approximately 70\%) than for highlands (approximately 50\%). This hydration loss and return to a steady state occurred entirely between local morning and evening, requiring a ready daytime source of water-group ions, which is consistent with a solar wind origin.},
author = {Sunshine, Jessica M and Farnham, Tony L and Feaga, Lori M and Groussin, Olivier and Merlin, Fr\'{e}d\'{e}ric and Milliken, Ralph E and A'Hearn, Michael F},
