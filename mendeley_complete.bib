month = aug,
number = {10},
pages = {1207--1214},
title = {{Concatenation of HRSC colour and OMEGA data for the determination and 3D-parameterization of high-altitude CO2 clouds in the Martian atmosphere}},
type = {Journal article},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0032063310001170},
volume = {58},
