journal = {Icarus},
keywords = {asteroids,ceres,photometry,surfaces},
mendeley-tags = {ceres},
month = may,
number = {1},
pages = {143--160},
title = {{Photometric analysis of 1 Ceres and surface mapping from HST observations}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0019103506000054},
volume = {182},
