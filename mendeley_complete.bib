keywords = {accepted,charon,ice,pluto,spectroscopy},
mendeley-tags = {accepted,charon,ice,pluto,spectroscopy},
month = aug,
publisher = {Elsevier Inc.},
title = {{Chemical and physical properties of the variegated Pluto and Charon surfaces}},
